META_START
    name: local
    parameters: iterations 
    csockets: 
    epr_sockets: 
META_END

^b0 {type = CL}:
    neg_one = assign_cval() : -1
    zero = assign_cval() : 0

    // When we run the prepare subroutine we also run one iteration of gates
    iterations = add_cval_c(iterations, neg_one)

^b1 {type = QL}:
    run_subroutine() : prepare

^b2 {type = QL}:
    run_subroutine() : gates

^b3 {type = CL}:
    iterations = add_cval_c(iterations, neg_one)
    bne(iterations,zero) : b2

^b4 {type = QL}:
    tuple<outcome> = run_subroutine() : measure

^b5 {type = CL}:
    return_result(outcome)

SUBROUTINE prepare
    params:
    returns: 
    uses: 0
    keeps: 0
    request: 
  NETQASM_START
    set Q0 0
    init Q0
    h Q0
    z Q0
    x Q0
    z Q0
    x Q0
    h Q0 
  NETQASM_END

SUBROUTINE gates
    params:
    returns: 
    uses: 0
    keeps: 0
    request: 
  NETQASM_START
    set Q0 0
    h Q0
    z Q0
    x Q0
    z Q0
    x Q0
    h Q0 
  NETQASM_END

SUBROUTINE measure
    params:
    returns: o
    uses: 0
    keeps: 
    request: 
  NETQASM_START
    set Q0 0
    meas Q0 M0
    store M0 @output[0]
  NETQASM_END