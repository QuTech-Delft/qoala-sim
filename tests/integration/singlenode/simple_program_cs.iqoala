META_START
    name: alice
    parameters: 
    csockets: 
    epr_sockets: 
    critical_sections: 0 -> AE
META_END

^b0 {type = QL, critical_section = 0}:
    run_subroutine() : subrt0

^b1 {type = CL, critical_section = 0}:
    x = assign_cval() : 0

^b2 {type = QL, critical_section = 0}:
    tuple<m> = run_subroutine() : subrt1

^b3 {type = CL}:
    return_result(m)

SUBROUTINE subrt0
    params:
    returns:
    uses: 0
    keeps: 0
    request: 
  NETQASM_START
    set Q0 0
    init Q0
    x Q0
  NETQASM_END

SUBROUTINE subrt1
    params:
    returns: m
    uses: 0
    keeps:
    request: 
  NETQASM_START
    meas Q0 M0
    store M0 @output[0]
  NETQASM_END